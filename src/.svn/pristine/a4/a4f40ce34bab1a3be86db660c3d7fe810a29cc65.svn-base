<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <div class="wrapper">
           
            <div class="login">
                <div class="container">
                    <div class="row justify-content-center">
                        <div class="col-md-12">
                            <div class="loginto ">
                                <form class="example-container" [formGroup]="registrationForm" *ngIf="registerForm">
                                    <h1 style="text-align: center; font-family: 'Great Vibes',cursive;">Registration</h1>
                                    <mat-form-field class="example-full-width">
                                        <input matInput placeholder="Name" 
                                               formControlName="REGISTERATION_NAME"
                                               [(ngModel)]="registrationModel.fullName" required/>
                                        <mat-error *ngIf="registrationForm.get('REGISTERATION_NAME').invalid && registrationForm.get('REGISTERATION_NAME').touched">{{validName()}}</mat-error>
                                    </mat-form-field>
                                    
                                    <mat-form-field class="example-full-width">
                                        <input matInput placeholder="Email"
                                               formControlName="REGISTERATION_EMAIL_ID" 
                                               [(ngModel)]="registrationModel.emailId" required/>
                                        <mat-error *ngIf="registrationForm.get('REGISTERATION_EMAIL_ID').invalid && registrationForm.get('REGISTERATION_EMAIL_ID').touched">{{validEmail()}}</mat-error>
                                    </mat-form-field>

                                    <mat-form-field class="example-full-width">
                                        <input matInput placeholder="Password" [type]="hide ? 'password' : 'text'"
                                               formControlName="REGISTERATION_PASSWORD" 
                                               [(ngModel)]="registrationModel.password" required/>
                                        <i matSuffix class="fa fa-eye-slash" *ngIf="hide===true" (click)="hide = !hide"></i>
                                        <i matSuffix class="fa fa-eye" *ngIf="hide===false" (click)="hide = !hide"></i>
                                        <mat-error *ngIf="registrationForm.get('REGISTERATION_PASSWORD').invalid && registrationForm.get('REGISTERATION_PASSWORD').touched">{{validPassword()}}</mat-error>
                                    </mat-form-field>
                                    
                                    <mat-form-field class="example-full-width">
                                        <span matPrefix>{{callingCode}}</span>&nbsp;
                                        <input type="tel" matInput placeholder="Mobile number" maxlength="10" 
                                               formControlName="REGISTERATION_MOBILE_NO"
                                               [(ngModel)]="registrationModel.mobileNo" required/>
                                        <mat-error *ngIf="registrationForm.get('REGISTERATION_MOBILE_NO').invalid && registrationForm.get('REGISTERATION_MOBILE_NO').touched">{{validMobile()}}</mat-error>
                                    </mat-form-field>
                                    <br>
                                    <div class="row" align="center">
                                        <div class="col-md-12" *ngIf="loadingRegistration==false">
                                    <button mat-raised-button color="warn" style="width: 100%" [disabled]="registrationForm.invalid" (click)="registration(registrationModel)">Register</button>
                                    </div>
                                        <div class="col-md-12" *ngIf="loadingRegistration==true">
                                         <button  mat-raised-button  style="width: 100%;background-color: #4CAF50"><i  class="fa fa-circle-o-notch fa-spin"></i>&nbsp;Loading</button>
                                        </div>
                                    </div>
                                    <br>
                                    <p style="text-align: center; color: red;font-weight: 700;cursor:pointer;cursor:hand;" (click)="openLogin()">I have an account</p>
                                    
                                </form>
                                <form class="example-container" [formGroup]="loginForm" *ngIf="!registerForm">
                                    <h1 style="text-align: center; font-family: 'Great Vibes',cursive;">Login</h1>
                                    <mat-form-field class="example-full-width">
                                        <input matInput placeholder="Email"
                                               formControlName="LOGIN_EMAIL_ID" 
                                               [(ngModel)]="loginModel.emailId" required/>
                                        <mat-error *ngIf="loginForm.get('LOGIN_EMAIL_ID').invalid && loginForm.get('LOGIN_EMAIL_ID').touched">{{validLoginEmail()}}</mat-error>
                                    </mat-form-field>

                                    <mat-form-field class="example-full-width">
                                        <input matInput placeholder="Password" [type]="hide ? 'password' : 'text'"
                                               formControlName="LOGIN_PASSWORD" 
                                               [(ngModel)]="loginModel.password" required/>
                                        <i matSuffix class="fa fa-eye-slash" *ngIf="hide===true" (click)="hide = !hide"></i>
                                        <i matSuffix class="fa fa-eye" *ngIf="hide===false" (click)="hide = !hide"></i>
                                        <mat-error *ngIf="loginForm.get('LOGIN_PASSWORD').invalid && loginForm.get('LOGIN_PASSWORD').touched">{{validLoginPassword()}}</mat-error>
                                    </mat-form-field>
                                    
                                    <br>
                                    <div class="row" align="center">
                                        <div class="col-md-12" *ngIf="loadingLogin==false">
                                            <button mat-raised-button color="warn" style="width: 100%;" [disabled]="loginForm.invalid" (click)="login(loginModel)">Login</button>
                                        </div>
                                        <div class="col-md-12" *ngIf="loadingLogin==true">
                                         <button  mat-raised-button  style="width: 100%;background-color: #4CAF50;color:white"><i  class="fa fa-circle-o-notch fa-spin"></i>&nbsp;Loading</button>
                                        </div>

                                    </div>
                                    
                                    <br>
                                    
                                    <div class="row">
                                        <div class="col-md-6">
                                            <button mat-raised-button color="warn" style="width: 100%; background: #4e71a7;" (click)="callFacebook()"><i class="icofont icofont-social-facebook"></i> Facebook</button>
                                        </div>
                                        
                                        <div class="col-md-6">
                                            <button mat-raised-button color="warn" style="width: 100%; background: #e44120;" (click)="callGoogle()"><i class="icofont icofont-social-google-plus"></i> Google+</button>
                                        </div>
                                    </div>
                                    
                                    
<!--                                    <div class="row">
                                        <div class="col-md-6">
                                            <button mat-raised-button color="warn" style="width: 100%; background: #e44120;" (click)="callGoogle()"><i class="icofont icofont-social-google-plus"></i> Login with Google+</button>
                                        </div>
                                    </div>-->
                                    <br>
                                    <div class="row" align="center">
                                        <div class="col-md-12">
                                            <p style='color:red;cursor:pointer;cursor:hand;font-weight: 700' (click)="openRegistration()">Don't have an account? </p>
                                            <!--<button mat-raised-button color="warn" style="width: 100%;" (click)="openRegistration()">Register</button>-->
                                        </div>
                                    </div>
                                    
                                    
                                </form>
                            </div>
                        </div>
                        <div class="col-lg-10 col-md-12">
                        </div>
                    </div>
                </div>
            </div>
            
           
        </div>