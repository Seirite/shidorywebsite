.example-container {
  display: flex;
  flex-direction: column;
}

.example-container > * {
  width: 100%;
}

.buttonload {
  background-color: #4CAF50; /* Green background */
  border: none; /* Remove borders */
  color: white; /* White text */
  padding: 12px 16px; /* Some padding */
  font-size: 16px /* Set a font size */
}

.divInner{
    left: 0;
    position: sticky;
    display: flex;
}

.divOuter{
    width:100%; 
    overflow:hidden
}
.marginTop{
    justify-content: center;
    display: flex;
}

.inputBorder{
    border-bottom: 4px solid #D9D9D9;
    margin-right: 10px;
    width: 30px;
    font-size: 24px;
}
::ng-deep .mat-horizontal-stepper-header{
    pointer-events: none !important;
}

.example-form {
    min-width: 150px;
    max-width: 500px;
    width: 100%;
}

.example-full-width {
    width: 100%;
}
